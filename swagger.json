    "components": {
        "schemas": {
            "CreateUserDto": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "age": {
                        "type": "number",
                        "minimum": 0
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "role": {
                        "type": "string",
                        "description": "role type",
                        "enum": [
                            "Admin",
                            "Manager",
                            "User"
                        ]
                    },
                    "status": {
                        "type": "string",
                        "description": "status type",
                        "enum": [
                            "Online",
                            "Offline"
                        ]
                    }
                },
                "required": [
                    "id",
                    "name",
                    "age",
                    "email",
                    "role",
                    "status"
                ]
            },
            "UpdateUserDto": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "age": {
                        "type": "number",
                        "minimum": 0
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "role": {
                        "type": "string",
                        "description": "role type",
                        "enum": [
                            "Admin",
                            "Manager",
                            "User"
                        ]
                    },
                    "status": {
                        "type": "string",
                        "description": "status type",
                        "enum": [
                            "Online",
                            "Offline"
                        ]
                    }
                }
            },
            "CreateCompanyDto": {
                "type": "object",
                "properties": {
                    "company_code": {
                        "type": "string",
                        "example": "C01"
                    },
                    "company_name": {
                        "type": "string",
                        "example": "Acme Corporation"
                    },
                    "level": {
                        "type": "number",
                        "example": 2,
                        "description": "Hierarchy level of the company"
                    },
                    "country": {
                        "type": "string",
                        "example": "USA"
                    },
                    "city": {
                        "type": "string",
                        "example": "San Francisco"
                    },
                    "founded_year": {
                        "type": "number",
                        "example": 1999
                    },
                    "annual_revenue": {
                        "type": "number",
                        "example": 1200000
                    },
                    "employees": {
                        "type": "number",
                        "example": 250
                    }
                },
                "required": [
                    "company_code",
                    "company_name",
                    "level",
                    "country",
                    "city",
                    "founded_year",
                    "annual_revenue",
                    "employees"
                ]
            },
            "CreateRelationDto": {
                "type": "object",
                "properties": {
                    "company_code": {
                        "type": "string",
                        "example": "C123"
                    },
                    "parent_company": {
                        "type": "string",
                        "example": "C1234"
                    }
                },
                "required": [
                    "company_code",
                    "parent_company"
                ]
            },
            "CreateAccountDto": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "alice@example.com"
                    },
                    "password": {
                        "type": "string",
                        "example": "s3cr3t",
                        "format": "password"
                    },
                    "phone": {
                        "type": "string",
                        "example": "+1-555-5555"
                    },
                    "address": {
                        "type": "string"
                    },
                    "country": {
                        "type": "string"
                    },
                    "state": {
                        "type": "string"
                    },
                    "city": {
                        "type": "string"
                    },
                    "postcode": {
                        "type": "string"
                    },
                    "about": {
                        "type": "string"
                    }
                },
                "required": [
                    "email",
                    "password"
                ]
            },
            "UpdateAccountDto": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
