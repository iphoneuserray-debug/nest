                "responses": {
                    "200": {
                        "description": "Returns the updated company object",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CreateCompanyDto"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request body or parameters"
                    }
                },
                "tags": [
                    "companies"
                ]
            }
        },
